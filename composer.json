{
    "name": "conjecto/nemrod",
    "description": "Nemrod is a framework providing an abstraction layer for handling (consuming and producing) RDF in a Symfony2 project",
    "type": "library",
    "keywords": ["RDF", "Semantic Web", "Linked Data", "Turtle", "RDFa", "SPARQL", "JsonLD"],
    "homepage": "http://www.conjecto.com",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Conjecto",
            "homepage": "http://wwww.conjecto.com",
            "email": "contact@conjecto.com"
        }
    ],
    "repositories" : [
      {
        "type": "package",
        "package": {
          "name": "conjecto/json-ld",
          "version": "1.1",
          "source": {
            "type" : "git",
            "url" : "git@github.com:conjecto/JsonLD.git",
            "reference" : "1.1"
          },
          "dist": {
            "url": "https://github.com/conjecto/JsonLD/archive/master.zip",
            "type": "zip"
          }
        }
      }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "~2.3",
        "easyrdf/easyrdf": "dev-master",
		"conjecto/json-ld": "~1.0",
        "jms/serializer": "~0.11",
        "ruflin/elastica": "~1.4.3"
    },
    "suggest": {
        "jms/serializer-bundle": "~0.13"
    },
    "require-dev": {
        "jms/serializer-bundle": "~0.13",
        "phpunit/phpunit": "4.3.*",
        "sensio/framework-extra-bundle": "~3.0,>=3.0.2",
        "fabpot/php-cs-fixer": "~1.5"
    },
    "autoload": {
        "psr-4": {
            "Conjecto\\Nemrod\\": "src/Conjecto/Nemrod/"
        }
    },
    "minimum-stability": "stable"
}
